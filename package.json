{
  "name": "cyclops",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "nx serve",
    "start:customer": "nx serve web-customer",
    "build": "nx build",
    "test": "nx test",
    "lint": "nx workspace-lint && nx lint",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "affected:lint": "nx affected:lint",
    "setup:husky": "husky install && rm -rf .git/hooks && ln -s ../.husky .git/hooks",
    "import-sort": "import-sort",
    "import-sort:check": "import-sort -l apps/**/*.ts{,x} libs/**/*.ts{,x}",
    "import-sort:write": "import-sort --write apps/**/*.ts{,x} libs/**/*.ts{,x}",
    "cspell": "cspell",
    "cspell:check": "cspell apps/**/*.{js,ts,tsx,json,html,less} libs/**/*.{js,ts,tsx,json,html,less}",
    "build:prod": "nx run $NX_APP_NAME:build:$NX_STAGE",
    "lint-staged": "lint-staged"
  },
  "private": true,
  "devDependencies": {
    "@nrwl/cli": "12.10.0",
    "@nrwl/cypress": "12.10.0",
    "@nrwl/eslint-plugin-nx": "12.10.0",
    "@nrwl/jest": "12.10.0",
    "@nrwl/linter": "12.10.0",
    "@nrwl/nest": "^12.10.0",
    "@nrwl/react": "^12.10.0",
    "@nrwl/tao": "12.10.0",
    "@nrwl/web": "12.10.0",
    "@nrwl/workspace": "12.10.0",
    "@testing-library/react": "11.2.6",
    "@testing-library/react-hooks": "7.0.1",
    "@types/jest": "27.0.2",
    "@types/node": "14.14.33",
    "@types/react": "17.0.3",
    "@types/react-dom": "17.0.3",
    "@types/react-router-dom": "^5.3.1",
    "@typescript-eslint/eslint-plugin": "~4.31.1",
    "@typescript-eslint/parser": "~4.31.1",
    "babel-jest": "27.2.3",
    "cspell": "^5.12.3",
    "cypress": "^8.3.0",
    "eslint": "7.22.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.23.1",
    "eslint-plugin-react-hooks": "4.2.0",
    "husky": "^7.0.2",
    "import-sort-cli": "^6.0.0",
    "import-sort-config": "^6.0.0",
    "import-sort-parser-typescript": "^6.0.0",
    "import-sort-style-module": "^6.0.0",
    "jest": "27.2.3",
    "lint-staged": "^11.2.3",
    "prettier": "^2.3.1",
    "react-scripts": "^4.0.3",
    "ts-jest": "27.0.5",
    "typescript": "~4.3.5"
  },
  "dependencies": {
    "antd": "^4.16.13",
    "axios": "^0.23.0",
    "core-js": "^3.6.5",
    "lodash": "^4.17.21",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-redux": "^7.2.6",
    "react-refresh": "^0.10.0",
    "react-router-dom": "^5.3.0",
    "react-svg": "^14.0.17",
    "redux": "^4.1.1",
    "redux-saga": "^1.1.3",
    "redux-thunk": "^2.3.0",
    "regenerator-runtime": "0.13.7",
    "tslib": "^2.0.0"
  }
}
